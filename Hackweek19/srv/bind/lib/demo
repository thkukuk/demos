;
; DNS table for demo
;
$TTL 1W
@       IN      SOA     rpi4-1.demo.     hostmaster.demo. (
                                2020021300      ; Serial
                                10800           ; Refresh
                                3600            ; Retry
                                1W              ; Expire
                                86400 )         ; Minimum

                IN      NS      rpi4-1.demo.

; fixed nodes
rpi4-1          IN      A       172.27.0.1
rpi4-2          IN      A       172.27.0.2
rpi4-3          IN      A       172.27.0.3
rpi4-4          IN      A       172.27.0.4

; loadbalancers for kubernetes
rpi4-lb1	IN	A	172.27.0.21
rpi4-lb2	IN	A	172.27.0.22
rpi4-lb3	IN	A	172.27.0.23
rpi4-lb4	IN	A	172.27.0.24
rpi4-lb5	IN	A	172.27.0.25
rpi4-lb6	IN	A	172.27.0.26
rpi4-lb7	IN	A	172.27.0.27
rpi4-lb8	IN	A	172.27.0.28
rpi4-lb9	IN	A	172.27.0.29

; dynamic IP addresses
